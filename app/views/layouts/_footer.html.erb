<footer>
  <div class="container">
    <div class="row">
    	<div class="col-md-3 col-xs-6">
	    	<ul class="list-unstyled footer-links">
	    		<% Settings.get['footer']['links'].each do |link| %>
            <% if link[1]['title'].present? %>
              <li><%= link_to link[1]['title'], link[1]['link'], target: '_blank' %></li>
            <% end %>
          <% end %>
	    	</ul>
	    </div>
      <div class="col-md-3 col-md-push-6 col-xs-6 text-right"><%= link_to "Back to Top", '#top' %></div>
	    <div class="col-md-4 col-xs-12 col-md-pull-3">
	    	<div class="row">
	    		<div class="col-md-4">Newsletter</div>
	    		<div class="col-md-8">
            <div id="mc_embed_signup">
              <form action="//space10.us12.list-manage.com/subscribe/post?u=a168b507fc8033ce61a467329&amp;id=35b5972dbe" method="post" id="mc-embedded-subscribe-form" name="mc-embedded-subscribe-form" class="validate" target="_blank" novalidate>
                  <div id="mc_embed_signup_scroll">
                
                <div class="mc-field-group form-group">
                  <!-- <label for="mce-FNAME">First Name </label> -->
                  <input type="text" value="" name="FNAME" class="required form-control" id="mce-FNAME" placeholder="First name">
                </div>
                <div class="mc-field-group form-group">
                  <!-- <label for="mce-EMAIL">Email Address </label> -->
                  <input type="email" value="" name="EMAIL" class="required form-control email" id="mce-EMAIL" placeholder="Email">
                </div>
                  <div style="position: absolute; left: -5000px;"><input type="text" name="b_a168b507fc8033ce61a467329_35b5972dbe" tabindex="-1" value=""></div>
                  <div class="clear"><input type="submit" value="Subscribe" name="subscribe" id="mc-embedded-subscribe" class="button btn btn-block btn-default"></div>
                  </div>
              </form>
            </div>
	    		</div>
	    	</div>
	    </div>
	    <div class="col-md-2 col-md-pull-3"></div>
	    
    </div>
    <div class="row row-same-height">
    	<div class="col-md-6 col-md-push-6 col-md-height">
  			<div class="space-footer-info">
  				<%= image_tag 'logos/text-logo-white.svg', width: "135" %>
  				<p>
            FlÃ¦sketorvet 10<br>
            1711 Copenhagen V<br>
            Denmark    
          </p>
  			</div>
    	</div>
      <div class="col-md-6 col-md-pull-6 col-md-height">
        <div class="social-media ">
          <ul class="list-unstyled list-inline">
            <% Settings.get['social_media'].each do |k,v| %>
              <% if v.present? %>
                <li><%= link_to "<i class='fa fa-lg fa-fw fa-#{k}'></i>".html_safe, "#{v}", target: '_blank' %></li>
              <% end %>
            <% end %>
          </ul>  
        </div>
      </div>
    </div>
  </div>
</footer>